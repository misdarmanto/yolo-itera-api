{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "@typescript-eslint/indent": "error",
    "space-before-function-paren": "off",
    "@typescript-eslint/space-before-function-paren": "error"
  }
}
